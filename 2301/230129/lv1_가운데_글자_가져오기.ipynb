    "import math\n",
    "\n",
    "def solution(s):\n",
    "    # 홀수\n",
    "    if len(s) % 2:\n",
    "        middle_index = math.ceil(len(s) / 2) - 1\n",
    "        return s[middle_index]\n",
    "    # 짝수\n",
    "    else:\n",
    "        middle_index = int(len(s) / 2) - 1\n",
    "        return s[middle_index : middle_index+2]"
   ]
  }
