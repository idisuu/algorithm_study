    "from collections import Counter\n",
    "\n",
    "def solution(string):\n",
    "    counter = Counter(string.lower())\n",
    "    cnt_p = counter['p']\n",
    "    cnt_y = counter['y']\n",
    "    if cnt_p == cnt_y:\n",
    "        return True\n",
    "    return False"
   ]
  }
