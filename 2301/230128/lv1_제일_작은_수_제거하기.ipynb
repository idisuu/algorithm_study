    "def solution(arr):\n",
    "    min_value = min(arr)\n",
    "    min_index = arr.index(min_value)\n",
    "    arr.pop(min_index)\n",
    "    if not arr:\n",
    "        arr.append(-1)\n",
    "    return arr"
