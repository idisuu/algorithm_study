    "def solution(absolutes, signs):\n",
    "    total = 0\n",
    "    for absolute, sign in zip(absolutes, signs):\n",
    "        if not sign:\n",
    "            sign = -1            \n",
    "        total += sign * absolute\n",
    "    return total"
   ]
  }
