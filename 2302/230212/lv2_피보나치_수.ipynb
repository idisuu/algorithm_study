    "def solution(n):\n",
    "    arr = [0 for _ in range(n + 1)]\n",
    "    arr[1] = 1\n",
    "    arr[2] = 1\n",
    "    for idx in range(3, n+1):\n",
    "        arr[idx] = arr[idx-2] + arr[idx-1]\n",
    "    return arr[n] % 1234567"
   ]
  }
