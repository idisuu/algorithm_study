    "    q = []\n",
    "    for ch in s:\n",
    "        if q and q[-1] == ch:\n",
    "            q.pop()\n",
    "            continue\n",
    "        q.append(ch)\n",
    "    if q:\n",
    "        return 0\n",
    "    else:\n",
    "        return 1"
