    "def solution(ingredient):\n",
    "    cnt = 0\n",
    "    target = [1, 2, 3, 1]\n",
    "    stack = []\n",
    "    for i in ingredient:\n",
    "        stack.append(i)\n",
    "        if stack[-4:] == target:\n",
    "            cnt += 1\n",
    "            for _ in range(4):\n",
    "                stack.pop()\n",
    "    return cnt"
   ]
  }
