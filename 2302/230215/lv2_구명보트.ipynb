   "source": [
    "def solution(people, limit):\n",
    "    sorted_people = sorted(people, reverse=True)\n",
    "    cnt = 0 \n",
    "    for weight in sorted_people:\n",
    "        if weight + sorted_people[-1] <= limit:\n",
    "            sorted_people.pop()\n",
    "        cnt += 1\n",
    "    return cnt"
   ]
  }
