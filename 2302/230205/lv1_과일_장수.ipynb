    "def solution(k, m, score):\n",
    "    total_box_score = 0\n",
    "    box = []\n",
    "    for num in sorted(score, reverse=True):\n",
    "        box.append(num)\n",
    "        if len(box) == m:\n",
    "            box_score = m*min(box)\n",
    "            total_box_score += box_score\n",
    "            box = []\n",
    "    return total_box_score"
   ]
  }
