    "def solution(n):\n",
    "    num = n\n",
    "    if num == 1:\n",
    "        return 1\n",
    "    cnt = 0\n",
    "    while True:\n",
    "        if num % 2 == 0:\n",
    "            num /= 2\n",
    "        else:\n",
    "            num -= 1\n",
    "            cnt += 1\n",
    "        if num == 1:\n",
    "            cnt += 1\n",
    "            return cnt"
   ]
