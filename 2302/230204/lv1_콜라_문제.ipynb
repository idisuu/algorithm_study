   "source": [
    "def solution(a, b, n):\n",
    "    cnt = 0\n",
    "    while True:\n",
    "        temp_cnt = n // a\n",
    "\n",
    "        if not temp_cnt:\n",
    "            break\n",
    "        cnt += (b*temp_cnt)\n",
    "        n -= (a*temp_cnt)\n",
    "        n += (b*temp_cnt)\n",
    "    return cnt"
   ]
  }
