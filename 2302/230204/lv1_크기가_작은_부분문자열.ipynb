    "def solution(t, p):\n",
    "    iter_cnt = len(t) - len(p) + 1\n",
    "    cnt = 0\n",
    "    for i in range(iter_cnt):\n",
    "        substring = int(t[i:i+len(p)])\n",
    "        if substring <= int(p):\n",
    "            cnt += 1\n",
    "    return cnt"
   ]
  }
