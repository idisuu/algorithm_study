   "source": [
    "from collections import deque\n",
    "\n",
    "def solution(s):\n",
    "    q = deque([])\n",
    "    for i in s:\n",
    "        if i == '(':\n",
    "            q.append('(')\n",
    "        else:\n",
    "            if not q:\n",
    "                return False\n",
    "            q.popleft()\n",
    "    if q:\n",
    "        return False\n",
    "    return True"
   ]
